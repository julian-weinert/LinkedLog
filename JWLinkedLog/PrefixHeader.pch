#ifndef LLog
 #define LLogF(format, ...) NSLog(@"%s:%i: %@", __FILE__, __LINE__, [NSString stringWithFormat:format, ##__VA_ARGS__]);

 #ifdef DEBUG
  #define LLog(format, ...) LLogF(format, ##__VA_ARGS__);
 #else
  #define LLog(format, ...) while(0){}
 #endif
